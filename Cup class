package liar;
import java.util.ArrayList;
/**
 *
 * @author Kaytlen
 */
public class cup {
    Dice yourdie = new Dice();
    ArrayList<Dice> diceNumber = new ArrayList<>();
    
    public void removeDice(Dice c)
    {
        diceNumber.remove();
    }
    
    public void addDice(Dice d)
    {
        diceNumber.add(d);
    }
    
}
